{
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "FileStorageFolder": "blob",
    "enyimMemcached": {
        "Servers": [
            {
                "Address": "memcached",
                "Port": 11211
            }
        ]
    },
    "CacheExpiration": 300,
    "ApiVersion": "1.0",
    "CacheProvider": "inmemorycaching"
}
